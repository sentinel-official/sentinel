{
  "name": "sentinel",
  "description": "Application for Sentinel",
  "productName": "Sentinel",
  "author": "Dev Team <devteam@sentinelgroup.io>",
  "version": "0.0.43",
  "private": true,
  "main": "electron.js",
  "license": "MIT",
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "https://github.com/sentinel-official/sentinel.git"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.3",
    "@material-ui/icons": "^2.0.1",
    "@material-ui/lab": "^1.0.0-alpha.10",
    "async": "^2.6.0",
    "axios": "^0.18.0",
    "child_process": "^1.0.2",
    "compose": "^0.1.2",
    "countrynames": "^0.1.1",
    "electron-hot-loader": "^0.8.0",
    "ip": "^1.1.5",
    "net-stat": "^2.0.1",
    "path": "^0.12.7",
    "react-proxy": "^1.1.8",
    "react-redux": "^5.0.7",
    "react-table-sorter": "^2.0.0",
    "react-text-mask": "^5.4.3",
    "redux": "^4.0.0",
    "redux-promise": "^0.6.0",
    "sudo-prompt": "^8.1.0",
    "url": "^0.11.0"
  },
  "scripts": {
    "start": "react-scripts build && sudo electron .",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "package-mac": "electron-packager . sentinel --overwrite --asar --platform=darwin --arch=x64 --icon=public/icon256x256.ico --prune=true --out=release-builds",
    "package-win": "electron-packager . sentinel --overwrite --asar --platform=win32 --icon=public/icon256x256.ico --prune=true --out=release-builds --win32metadata.CompanyName=Sentinel --win32metadata.FileDescription=\"Application for Sentinel\" --win32metadata.requested-execution-level=requireAdministrator --win32metadata.ProductName=\"Sentinel\"",
    "package-linux": "electron-packager . sentinel --overwrite --asar --platform=linux --arch=x64 --icon=public/icon256x256.png --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/sentinel-darwin-x64/sentinel.app sentinel-alpha-0.0.43 --out=release-builds --overwrite --icon=public/icon256x256.icns",
    "create-installer-win": "node installers/windows/createinstaller.js",
    "create-debian-installer": "electron-installer-debian --src release-builds/sentinel-linux-x64/ --arch amd64 --config debian.json"
  },
  "devDependencies": {
    "babel-runtime": "^6.26.0",
    "electron": "^1.8.7",
    "electron-installer-debian": "^0.8.1",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^10.1.2",
    "electron-reload": "^1.2.5",
    "electron-winstaller": "^2.6.3",
    "ethereumjs-tx": "^1.3.4",
    "keythereum": "^1.0.2",
    "material-ui": "^0.20.0",
    "raven-js": "^3.23.2",
    "react": "^16.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-flexbox-grid": "^2.0.0",
    "react-qr-svg": "^2.1.0",
    "react-scripts": "1.1.1",
    "react-simple-maps": "^0.11.2",
    "react-tooltip": "^3.4.0",
    "react-world-flags": "0.0.3",
    "web3": "^0.20.6",
    "zfill": "0.0.2"
  }
}
