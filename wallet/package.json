{
  "name": "sentinelwallet",
  "description": "Sentinel Wallet Desktop App",
  "productName": "Sentinel Wallet Desktop App",
  "version": "1.0.0",
  "private": true,
  "main": "electron.js",
  "homepage": "https://sentinelgroup.io",
  "dependencies": {
    "electron-packager": "^10.1.1",
    "fs": "0.0.1-security",
    "fs-thenable": "^1.1.1",
    "material-ui": "^0.20.0",
    "react": "^16.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-flexbox-grid": "^2.0.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "package-mac": "electron-packager . sentinel-wallet-app --overwrite --asar=true --platform=darwin --arch=x64 --icon=public/icon256x256.ico --prune=true --out=release-builds",
    "package-win": "electron-packager . sentinel-wallet-app --overwrite --asar=true --platform=win32 --icon=public/icon256x256.ico --prune=true --out=release-builds --version-string.CompanyName=Sentinel --version-string.FileDescription=S --version-string.ProductName=\"Sentinel Wallet\"",
    "package-linux": "electron-packager . sentinel-wallet-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/sentinel-wallet-app-darwin-x64/Sentinel\\ wallet\\ app.app sentinel-desktop-app --out=release-builds --overwrite --icon=assets/icons/mac/icon.icns",
    "create-installer-win": "node installers/windows/createinstaller.js",
    "create-debian-installer": "electron-installer-debian --src release-builds/sentinel-wallet-app-linux-x64/ --arch amd64 --config debian.json"
  },
  "devDependencies": {
    "electron": "1.7.10",
    "electron-winstaller": "^2.6.3"
  }
}
